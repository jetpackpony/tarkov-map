{
  "name": "tarkov",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "rm -rf .parcel-cache/ .cache/ && parcel index.html",
    "build-main": "rm -rf .cache/ dist/ .parcel-cache/ && OFFLINE=false DB_COLLECTION_NAME=mapObjects parcel build index.html --no-source-maps && mkdir -p reports && mv dist/report.html reports/",
    "build-demo": "rm -rf .cache/ dist/ .parcel-cache/ && DB_COLLECTION_NAME=mapObjectsDemo parcel build index.html --no-source-maps && mkdir -p reports && mv dist/report.html reports/",
    "deploy-main": "firebase deploy --only hosting:main",
    "deploy-demo": "firebase deploy --only hosting:demo",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "local": "firebase serve --only hosting",
    "test-firebase": "parcel tests/index-firebase.test.html"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/plugin-transform-react-jsx": "^7.12.7",
    "@storybook/addon-actions": "^5.3.21",
    "@storybook/addon-links": "^5.3.21",
    "@storybook/addons": "^5.3.21",
    "@storybook/preact": "^5.3.21",
    "babel-loader": "^8.2.2",
    "cssnano": "^4.1.10",
    "parcel-bundler": "^1.12.4",
    "parcel-plugin-bundle-visualiser": "^1.2.0"
  },
  "dependencies": {
    "firebase": "^7.24.0",
    "i18next": "^19.8.4",
    "i18next-browser-languagedetector": "^4.3.1",
    "nanoid": "^2.1.11",
    "preact": "^10.5.7",
    "rambda": "^6.4.0",
    "react-color": "^2.19.3",
    "react-i18next": "^11.7.3",
    "react-redux": "^7.2.2",
    "redux": "^4.0.5",
    "redux-logger": "^3.0.6"
  },
  "alias": {
    "react": "preact/compat",
    "react-dom/test-utils": "preact/test-utils",
    "react-dom": "preact/compat"
  }
}
